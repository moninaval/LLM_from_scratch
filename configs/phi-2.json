{
  "model_type": "phi-2",
  "tokenizer_type": "huggingface",
  "tokenizer_path": "microsoft/phi-2",
  "vocab_size": 51200,
  "max_position_embeddings": 2048,
  "hidden_size": 2560,
  "num_attention_heads": 32,
  "num_hidden_layers": 32,
  "intermediate_size": 10240,
  "dropout": 0.0,
