    "use_bias": false
  },

  "ffn": {
    "type": "standard",
    "activation": "gelu"
  },

  "norm": {
    "style": "pre"
  },

  "initializer_range": 0.02,
  "tie_word_embeddings": true,
  "use_flash_attention": false,

  "tokenizer_path": "microsoft/phi-2",
  "weights_path": "./pretrained/phi-2/pytorch_model.bin"
}
